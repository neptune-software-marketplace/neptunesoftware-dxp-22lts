					"fieldType": "neptune.Typescript",
					"script": [
						"==OBJECT STRING==",
						"namespace CustomComponent {",
						"    let signaturePad;",
						"    let id = ModelData.genID();",
						"    let canvas;",
						"",
						"    export function create({ backgroundColor = 'rgba(255, 255, 255)', penColor = 'rgb(0, 0, 0)' }) {",
						"        canvas = document.getElementById('cc-signature-pad') as HTMLCanvasElement;",
						"        if (!canvas) {",
						"            sap.m.MessageToast.show(\"Error creating signature pad\");",
						"            return;",
						"        }",
						"",
						"        canvas.id = id;",
						"",
						"        // @ts-ignore",
						"        signaturePad = new SignaturePad(canvas, {",
						"            backgroundColor,",
						"            penColor",
						"        });",
						"",
						"        const parent = canvas.parentElement;",
						"        canvas.style.width = '100%';",
						"        canvas.style.height = '100%';",
						"        canvas.width = parent.clientWidth;",
						"        canvas.height = parent.clientHeight;",
						"",
						"        signaturePad.clear();",
						"    }  ",
						"",
						"    export function exportSignature() {",
						"        if (signaturePad.isEmpty()) {",
						"            sap.m.MessageToast.show(\"Please sign!\");",
						"        } else {",
						"            return signaturePad.toDataURL();",
						"        }",
						"    }",
						"",
						"    export function clear() {",
						"        signaturePad.clear();",
						"    }",
						"",
						"    export function getId() {",
						"        return id;",
						"    }",
						"",
						"    export function getCanvas() {",
						"        return canvas;",
						"    }",
						"",
						"    export function getSignaturePad() {",
						"        return signaturePad;",
						"    }",
						"}",
						"",
						"window.CustomComponent = CustomComponent;"
					],
					"customComponent": false,
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				}
			]
